{
  "format_version": "1.8.0",
  "animation_controllers": {
    "controller.animation.vehiclezebra.move": {
      "states": {
        "default": {
          "transitions": [
            { "move": "query.modified_move_speed > 0.05" }
          ]
        },
        "move": {
          "animations": [ { "move": [] } ],
          "transitions": [
            { "default": "query.modified_move_speed < 0.05" }
          ]
        }
      }
    }
  }
}
